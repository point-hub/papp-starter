    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.0.0",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.11",
    "typescript": "~5.8.2",
    "unocss": "^66.0.0",
