    "eslint-plugin-vue": "^10.1.0",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.12",
    "typescript": "~5.8.3",
    "unocss": "^66.1.2",
